{
  "coc.source.file.ignoreHidden": false,
  "coc.preferences.rootPatterns": [".git", ".hg", ".projections.json", ".root"],
  "yank.enableCompletion": false,
  "suggest.enablePreview": true,
  "suggest.noselect": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.source": "vim-devicons",
  "explorer.file.showHiddenFiles": true,
  "explorer.keyMappings.global": {
    "i": false,

    "*": "toggleSelection",
    "<tab>": "actionMenu",

    "gk": ["wait", "expandablePrev"],
    "gj": ["wait", "expandableNext"],
    "h": ["wait", "collapse"],
    "l": ["wait", "expandable?", "expand", "open"],
    "J": ["wait", "toggleSelection", "normal:j"],
    "K": ["wait", "toggleSelection", "normal:k"],
    "gl": ["wait", "expand:recursive"],
    "gh": ["wait", "collapse:recursive"],
    "<2-LeftMouse>": [
      "expandable?",
      ["expanded?", "collapse", "expand"],
      "open"
    ],
    "o": ["wait", "expanded?", "collapse", "expand"],
    "<cr>": ["wait", "expandable?", "cd", "open"],
    "e": "open",
    "s": "open:split",
    "E": "open:vsplit",
    "t": "open:tab",
    "<bs>": ["wait", "gotoParent"],
    "gs": ["wait", "reveal:select"],
    "il": "preview:labeling",
    "ic": "preview:content",
    "Il": "previewOnHover:toggle:labeling",
    "Ic": "previewOnHover:toggle:content",
    "II": "previewOnHover:disable",

    "yp": "copyFilepath",
    "yn": "copyFilename",
    "yy": "copyFile",
    "dd": "cutFile",
    "p": "pasteFile",
    "df": "delete",
    "dF": "deleteForever",

    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",

    "zh": "toggleHidden",
    "g<dot>": "toggleHidden",
    "R": "refresh",

    "?": "help",
    "q": "quit",
    "<esc>": "esc",
    "X": "systemExecute",
    "gd": "listDrive",

    "f": "search",
    "F": "searchRecursive",

    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",

    "[[": ["wait", "sourcePrev"],
    "]]": ["wait", "sourceNext"],

    "[i": ["wait", "indentPrev"],
    "]i": ["wait", "indentNext"],

    "[m": ["wait", "markPrev:modified"],
    "]m": ["wait", "markNext:modified"],

    "[d": ["wait", "markPrev:diagnosticError:diagnosticWarning"],
    "]d": ["wait", "markNext:diagnosticError:diagnosticWarning"],
    "[D": ["wait", "markPrev:diagnosticError"],
    "]D": ["wait", "markNext:diagnosticError"],

    "[c": ["wait", "markPrev:git"],
    "]c": ["wait", "markNext:git"],
    "<<": "gitStage",
    ">>": "gitUnstage"
  },
  "jedi.workspace.extraPaths": ["facial_recognition"],
  "rust-analyzer.diagnostics.disabled": [
    "unresolved-macro-call",
    "incorrect-ident-case",
    "missing-unsafe"
  ],

  // "rust-analyzer.checkOnSave.target": "thumbv7em-none-eabihf",
  // "rust-analyzer.checkOnSave.allTargets": false,
  "rust-analyzer.lens.methodReferences": true,
  "rust-analyzer.procMacro.enable": true,

  "rust-analyzer.lruCapacity": 1000,
  "rust-analyzer.updates.channel": "nightly",
  "go.goplsOptions": {
    "completeUnimported": true
  },
  "markdownlint.onOpen": false,
  "markdownlint.onChange": false,
  "markdownlint.onSave": false,
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "svlangserver": {
      "module": "/home/niuiic/Applications/Npm/npm_global/lib64/node_modules/@imc-trading/svlangserver/lib/svlangserver.js",
      "filetypes": ["systemverilog"],
      "settings": {
        "systemverilog.includeIndexing": [
          "sources*/**/*.{sv,svh}",
          "*.{sv,svh}"
        ],
        "systemverilog.excludeIndexing": ["sim*/**/*.sv*"],
        "systemverilog.defines": [],
        "systemverilog.launchConfiguration": "/usr/bin/verilator -sv -Wall --lint-only",
        "systemverilog.formatCommand": "/home/niuiic/Applications/Verible/verible/bin/verible-verilog-format"
      }
    },
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
      "rootPatterns": [
        ".root",
        ".ccls",
        "compile_commands.json",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    },
    "kotlin": {
      "command": "~/Applications/Kotlin/lsp/bin/kotlin-language-server",
      "filetypes": ["kotlin"]
    },
    "lsp-asm": {
      "command": "lsp-asm",
      "filetypes": ["asm", "s", "S"]
    }
  },
  "sourcekit.commandPath": "/home/niuiic/Applications/Swift/swift/usr/bin/sourcekit-lsp",
  "vetur.useWorkspaceDependencies": true,
  "tsserver.log": "verbose",
  "eslint.packageManager": "yarn",
  "eslint.nodePath": ".yarn/sdks",
  "snippets.userSnippetsDirectory": "/home/niuiic/.config/nvim/snippets",
  "translator.proxy": "socks://127.0.0.1:10024",
  "translator.toLang": "zh_CN",
  "cSpell.enabledLanguageIds": ["markdown"],
  "cSpell.userWords": [
    "Neovim",
    "Tauri",
    "Vimscript",
    "typeof"
  ]
}
